<h1>this is account category list page 123</h1>




<table class="table table-striped table-hover">
    <thead>
        <tr>
            @Html.ActionLink("Add","Edit")
        </tr>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.BookingID)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CheckInDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CheckOutDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NoOfAdult)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NoOfChildren)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TotalAmount)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Created)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Modified)
            </th>
            <th colspan="2" style="text-align: center">Action</th>
        </tr>
    </thead>
    <tbody id="new_2">
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.BookingID)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.CheckInDate)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.CheckOutDate)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.NoOfAdult)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.NoOfChildren)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.TotalAmount)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Created)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Modified)
                </td>
                

                <td>
                    <a href="@Url.Action("Edit","Booking",new {BookingID=item.BookingID})" class="btn btn-outline-primary">Edit</a>
                </td>
                <td>
                    <a href="@Url.Action("Delete","Booking",new {BookingID=item.BookingID})" class="btn btn-outline-danger" onclick="DeletePopUp()">
                        Delete
                    </a>
                </td>
            </tr>
        }
    </tbody>
</table>